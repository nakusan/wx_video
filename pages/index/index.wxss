.navtab_content_item {
  height: 170rpx;
  padding-top: 18rpx;
  background-color: white;
  padding: 10rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 3rpx solid #E0E0E0;
}
.navtab_content_item image {
  width: 290rpx;
  height: 170rpx;
  margin-right: 14rpx;
  flex-grow: 0;
  flex-shrink: 0;
  border-radius: 4px;
}
.navtab_content_item view {
  flex-grow: 1;
  flex-shrink: 1;
  font-size: 12px;
}